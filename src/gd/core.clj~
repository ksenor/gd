(ns gd.core)
(use 'clj-webdriver.taxi)

(defn fb-browser []
  (set-driver! {:browser :firefox} "https://fb.com"))

(defn fb-login [login_password_pair]
  (click "#email")
  (input-text "#email" (first login_password_pair))
  (click "#pass")
  (input-text "#pass" (second login_password_pair))
  (click "#u_0_n"))
